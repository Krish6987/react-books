{
  "name": "confusion",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cross-fetch": "^3.0.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "json-server": "^0.15.0",
    "pm2": "latest",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "npm start",
    "json-serve": "json-server --watch ss.json --host 0.0.0.0 --port 3001",
    "build-serve": "json-server --watch ss.json --host 0.0.0.0 --port 3001 & serve -s build -l 3000",
    "eject": "react-scripts eject",
    "test" : "CI=true react-scripts test",
    "test:ci": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "babel-jest": "latest",
    "jest": "24.7.1",
    "react-test-renderer": "latest",
    "jest-junit": "latest",
    "enzyme": "latest",
    "enzyme-adapter-react-16": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!**/node_modules/**",
      "!build/**"
      ],
    "coverageReporters":[
      "lcov",
      "text"
      ]
  }
}
